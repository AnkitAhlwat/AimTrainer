<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.termproject.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Pane fx:id = "pane" onMouseClicked="#changeLocation">
        <Circle fx:id="bullsEye" radius="20" visible="false" />
    </Pane>
    <Label fx:id="welcomeText"/>
    <Button fx:id="button" text="Hello!" onAction="#onHelloButtonClick"/>
</VBox>
